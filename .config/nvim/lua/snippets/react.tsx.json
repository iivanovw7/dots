    "prefix": "ufl",
    "body": [
      "useEffect(() => {",
      "  const handleFocus = () => {",
      "    const el = document.activeElement;",
      "    console.log('%c Focus Shifted ðŸ”', 'background: #222; color: #bada55; padding: 2px 5px; border-radius: 3px;', {",
      "      tag: el?.tagName,",
      "      id: el?.id || 'no-id',",
      "      class: el?.className || 'no-class',",
      "      label: el?.getAttribute('aria-label'),",
      "      // @ts-ignore",
      "      tabIndex: el?.tabIndex,",
      "      element: el",
      "    });",
      "  };",
      "",
      "  window.addEventListener('focusin', handleFocus);",
      "  return () => window.removeEventListener('focusin', handleFocus);",
      "}, []);"
    ],
    "description": "Logs the currently focused element to the console whenever it changes."
